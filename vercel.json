{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "@vite_supabase_url",
    "SUPABASE_SERVICE_KEY": "@supabase_service_key",
    "VITE_DOCUSIGN_INTEGRATION_KEY": "@vite_docusign_integration_key",
    "VITE_DOCUSIGN_USER_ID": "@vite_docusign_user_id",
    "VITE_DOCUSIGN_ACCOUNT_ID": "@vite_docusign_account_id",
    "VITE_DOCUSIGN_PRIVATE_KEY": "@vite_docusign_private_key",
    "VITE_DOCUSIGN_AUTH_SERVER": "@vite_docusign_auth_server"
  }
} 